MainWindow
  id: gameMessageWindow
  size: 350 100
  !text: tr("Info")

  Label
    id: label
    !text: tr('')
    text-wrap: true
    anchors.fill: parent

  Button
    id: submitButton
    !text: tr('Ok')
    anchors.right: parent.right
    anchors.bottom: parent.bottom
    @onClick: destroyMessageWindow()